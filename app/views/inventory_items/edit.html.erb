<div class="invt-txt">
<h1>Edit Inventory Item</h1>
</div>
<%= form_with(model: @inventory_item, data: { controller: "inventory-item" }) do |form| %>
  <div>
    <%= form.label :quantity %>
    <%= form.number_field :quantity %>
  </div>
  <div class="form-updt">
    <%= form.submit 'Update' %>
  </div>
<% end %>
